{
  "name": "@david.kucsai/react-pdf-table",
  "version": "0.4.1",
  "description": "Provides tables for the @react-pdf/renderer.",
  "main": "./lib/index.js",
  "scripts": {
    "generate-barrels": "barrelsby -d src --delete -e TableRow.tsx",
    "build": "rm -rf ./lib/* && npm run generate-barrels && tsc && npm run build-doco",
    "recompile": "tsc",
    "prepublish": "npm run build",
    "build-storybook": "build-storybook -o docs",
    "build-doco": "typedoc --plugin typedoc-plugin-markdown --excludeExternals --out docs/typedoc src"
  },
  "author": "David Kucsai",
  "license": "MIT",
  "peerDependencies": {
    "@react-pdf/renderer": "^3.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@react-pdf/renderer": "^3.1.12",
    "@types/enzyme": "^3.1.10",
    "@types/faker": "^4.1.5",
    "@types/node": "^13.1.8",
    "@types/sinon": "^4.3.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.0",
    "babel-preset-env": "^1.7.0",
    "barrelsby": "^2.1.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "faker": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.2",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.3",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^4.2.2",
    "webpack": "^4.46.0"
  },
  "resolutions": {
    "dfa": "1.1.0"
  },
  "files": [
    "lib",
    "LICENSE",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmk99/react-pdf-table.git"
  },
  "bugs": {
    "url": "https://github.com/dmk99/react-pdf-table/issues"
  },
  "homepage": "https://github.com/dmk99/react-pdf-table.git#README",
  "types": "./lib/index.d.ts",
  "dependencies": {}
}
